{
    "App.vue": {
        "content": "<!-- src/App.vue -->\n\n<template>\n  <v-app>\n    <AppNavbar />\n    <v-main>\n      <v-container fluid>\n        <router-view />\n      </v-container>\n    </v-main>\n    <AppFooter />\n  </v-app>\n</template>\n\n<script>\nimport AppNavbar from './components/Shared/AppNavbar.vue'\nimport AppFooter from './components/Shared/AppFooter.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    AppNavbar,\n    AppFooter,\n  },\n}\n</script>\n\n<style>\n/* \u5168\u5c40\u6837\u5f0f\uff08\u5982\u679c\u9700\u8981\uff09 */\n</style>\n",
        "from_file": "App.vue"
    },
    "pack.py": {
        "content": "import os\nimport json\n\nresult = {}\nfor root, dirs, files in os.walk('.'):\n    if 'node_modules' in dirs:\n        dirs.remove('node_modules')\n    for file in files:\n        file_path = os.path.join(root, file)\n        try:\n            with open(file_path, 'r') as f:\n                content = f.read()\n                relative_path = os.path.relpath(file_path, '.')\n                result[relative_path] = {\"content\": content, \"from_file\": relative_path}  # \u5c06\u6587\u4ef6\u5185\u5bb9\u548c\u6765\u6e90\u6587\u4ef6\u8def\u5f84\u90fd\u5b58\u5165\u5b57\u5178\uff0c\u952e\u5206\u522b\u4e3a'content'\u548c'from_file'\n        except Exception as e:\n            print(f\"Error reading {file_path}: {e}\")\n\nwith open('all_files.json', 'w') as f:\n    json.dump(result, f, indent=4)  # \u4f7f\u7528indent\u53c2\u6570\u8ba9\u751f\u6210\u7684JSON\u683c\u5f0f\u66f4\u6613\u8bfb",
        "from_file": "pack.py"
    },
    "main.js": {
        "content": "// src/main.js\n\nimport { createApp } from 'vue'\nimport App from './App.vue'\nimport router from './router'\nimport store from './store'\nimport vuetify from './plugins/vuetify' // \u5f15\u5165 Vuetify \u914d\u7f6e\nimport '@mdi/font/css/materialdesignicons.css' // \u5f15\u5165 MDI \u56fe\u6807\nimport './plugins/webfontloader' // \u5f15\u5165\u5b57\u4f53\u52a0\u8f7d\uff08\u5982\u679c\u6709\uff09\nimport apiClient from './services/api' // \u5f15\u5165 API \u670d\u52a1\n\nconst app = createApp(App)\n\napp.use(router)\napp.use(store)\napp.use(vuetify)\n\napp.mount('#app')\n\n// \u5728\u5e94\u7528\u542f\u52a8\u65f6\uff0c\u68c0\u67e5\u662f\u5426\u6709 userId\nconst userId = localStorage.getItem('userId')\nif (userId) {\n  store.dispatch('auth/fetchUser').catch(() => {\n    store.dispatch('auth/logout')\n  })\n}\n",
        "from_file": "main.js"
    },
    "store/index.js": {
        "content": "// src/store/index.js\n\nimport { createStore } from 'vuex'\nimport auth from './modules/auth'\nimport menu from './modules/menu'\nimport orders from './modules/orders'\n\nconst store = createStore({\n  modules: {\n    auth,\n    menu,\n    orders,\n  },\n})\n\nexport default store\n",
        "from_file": "store/index.js"
    },
    "store/modules/menu.js": {
        "content": "// src/store/modules/menu.js\n\nimport apiClient from '@/services/api'\n\nconst state = {\n  menuItems: [],\n}\n\nconst mutations = {\n  SET_MENU(state, menu) {\n    state.menuItems = menu\n  },\n  ADD_MENU_ITEM(state, item) {\n    state.menuItems.push(item)\n  },\n  UPDATE_MENU_ITEM(state, updatedItem) {\n    const index = state.menuItems.findIndex(item => item.item_id === updatedItem.item_id)\n    if (index !== -1) {\n      state.menuItems.splice(index, 1, updatedItem)\n    }\n  },\n  DELETE_MENU_ITEM(state, itemId) {\n    state.menuItems = state.menuItems.filter(item => item.item_id !== itemId)\n  },\n}\n\nconst actions = {\n  // \u83b7\u53d6\u83dc\u5355\n  async fetchMenu({ commit }) {\n    try {\n      const response = await apiClient.get('/menu')\n      commit('SET_MENU', response.data)\n    } catch (error) {\n      console.error('\u83b7\u53d6\u83dc\u5355\u5931\u8d25\uff1a', error)\n      throw error\n    }\n  },\n\n  // \u6dfb\u52a0\u83dc\u54c1\n  async addMenuItem({ commit }, newItem) {\n    try {\n      const response = await apiClient.post('/menu', newItem)\n      commit('ADD_MENU_ITEM', { ...newItem, item_id: response.data.item_id })\n    } catch (error) {\n      console.error('\u6dfb\u52a0\u83dc\u54c1\u5931\u8d25\uff1a', error)\n      throw error\n    }\n  },\n\n  // \u66f4\u65b0\u83dc\u54c1\n  async updateMenuItem({ commit }, updatedItem) {\n    try {\n      await apiClient.put(`/menu/${updatedItem.item_id}`, updatedItem)\n      commit('UPDATE_MENU_ITEM', updatedItem)\n    } catch (error) {\n      console.error('\u66f4\u65b0\u83dc\u54c1\u5931\u8d25\uff1a', error)\n      throw error\n    }\n  },\n\n  // \u5220\u9664\u83dc\u54c1\n  async deleteMenuItem({ commit }, itemId) {\n    try {\n      await apiClient.delete(`/menu/${itemId}`)\n      commit('DELETE_MENU_ITEM', itemId)\n    } catch (error) {\n      console.error('\u5220\u9664\u83dc\u54c1\u5931\u8d25\uff1a', error)\n      throw error\n    }\n  },\n}\n\nconst getters = {\n  getMenuItems: (state) => state.menuItems,\n}\n\nexport default {\n  namespaced: true,\n  state,\n  mutations,\n  actions,\n  getters,\n}\n",
        "from_file": "store/modules/menu.js"
    },
    "store/modules/orders.js": {
        "content": "// src/store/modules/orders.js\n\nconst state = {\n    orders: [],\n  }\n  \n  const mutations = {\n    SET_ORDERS(state, orders) {\n      state.orders = orders\n    },\n  }\n  \n  const actions = {\n    fetchOrders({ commit }) {\n      // \u540e\u7eed\u9636\u6bb5\u5b9e\u73b0\n    },\n  }\n  \n  const getters = {\n    allOrders: (state) => state.orders,\n  }\n  \n  export default {\n    namespaced: true,\n    state,\n    mutations,\n    actions,\n    getters,\n  }\n  ",
        "from_file": "store/modules/orders.js"
    },
    "store/modules/auth.js": {
        "content": "// src/store/modules/auth.js\n\nimport apiClient from '@/services/api'\nimport router from '@/router'\n\nconst state = {\n  user: null,\n  userId: localStorage.getItem('userId') || null,\n}\n\nconst mutations = {\n  SET_USER(state, user) {\n    state.user = user\n  },\n  SET_USER_ID(state, userId) {\n    state.userId = userId\n  },\n  CLEAR_AUTH(state) {\n    state.user = null\n    state.userId = null\n  },\n}\n\nconst actions = {\n  // \u7528\u6237\u6ce8\u518c\n  async register({ commit }, user) {\n    try {\n      const response = await apiClient.post('/users/register', user)\n      // \u6ce8\u518c\u6210\u529f\u540e\uff0c\u7528\u6237\u9700\u8981\u624b\u52a8\u767b\u5f55\n      return response\n    } catch (error) {\n      throw error\n    }\n  },\n\n  // \u7528\u6237\u767b\u5f55\n  async login({ commit, dispatch }, credentials) {\n    try {\n      const response = await apiClient.post('/users/login', credentials)\n      // \u540e\u7aef\u8fd4\u56de { message, user_id }\n      const { user_id } = response.data\n      commit('SET_USER_ID', user_id)\n      localStorage.setItem('userId', user_id)\n      // \u83b7\u53d6\u7528\u6237\u4fe1\u606f\n      await dispatch('fetchUser')\n    } catch (error) {\n      throw error\n    }\n  },\n\n  // \u7528\u6237\u767b\u51fa\n  logout({ commit }) {\n    commit('CLEAR_AUTH')\n    localStorage.removeItem('userId')\n    router.push('/login')  // \u767b\u51fa\u540e\u8df3\u8f6c\u5230\u767b\u5f55\u9875\u9762\n  },\n\n  // \u83b7\u53d6\u5f53\u524d\u7528\u6237\u4fe1\u606f\n  async fetchUser({ commit, state }) {\n    try {\n      if (state.userId) {\n        const response = await apiClient.get(`/users/${state.userId}`)\n        commit('SET_USER', response.data)\n      }\n    } catch (error) {\n      commit('CLEAR_AUTH')\n      throw error\n    }\n  },\n}\n\nconst getters = {\n  isAuthenticated: (state) => !!state.userId,\n  getUser: (state) => state.user,\n}\n\nexport default {\n  namespaced: true,\n  state,\n  mutations,\n  actions,\n  getters,\n}\n",
        "from_file": "store/modules/auth.js"
    },
    "services/api.js": {
        "content": "// src/services/api.js\n\nimport axios from 'axios'\n\n// \u521b\u5efa Axios \u5b9e\u4f8b\nconst apiClient = axios.create({\n  baseURL: 'http://localhost:5000/api', // \u786e\u4fdd baseURL \u5305\u542b /api\n  headers: {\n    'Content-Type': 'application/json',\n  },\n})\n\n// \u79fb\u9664 Authorization \u76f8\u5173\u7684\u62e6\u622a\u5668\uff0c\u56e0\u4e3a\u60a8\u4e0d\u4f7f\u7528 JWT\n/*\napiClient.interceptors.request.use(\n  (config) => {\n    const token = localStorage.getItem('token')\n    if (token) {\n      config.headers.Authorization = `Bearer ${token}`\n    }\n    return config\n  },\n  (error) => {\n    return Promise.reject(error)\n  }\n)\n*/\n\nexport default apiClient\n",
        "from_file": "services/api.js"
    },
