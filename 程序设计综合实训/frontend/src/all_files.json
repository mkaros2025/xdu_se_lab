    "views/ReviewsPage.vue": {
        "content": "<!-- src/views/ReviewsPage.vue -->\n\n<template>\n  <v-container>\n    <h1>kkk\u9910\u5385\u8bc4\u4ef7</h1>\n    <!-- \u8bc4\u4ef7\u5217\u8868\u5c06\u5728\u540e\u7eed\u9636\u6bb5\u5b9e\u73b0 -->\n  </v-container>\n</template>\n\n<script>\nexport default {\n  name: 'ReviewsPage',\n}\n</script>\n\n<style scoped>\n/* \u6dfb\u52a0\u6837\u5f0f\uff08\u5982\u679c\u9700\u8981\uff09 */\n</style>\n",
        "from_file": "views/ReviewsPage.vue"
