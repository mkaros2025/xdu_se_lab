    "views/LoginPage.vue": {
        "content": "<!-- src/views/LoginPage.vue -->\n\n<template>\n  <UserLogin />\n</template>\n\n<script>\nimport UserLogin from '@/components/Auth/UserLogin.vue'\n\nexport default {\n  name: 'LoginPage',\n  components: {\n    UserLogin\n  }\n}\n</script>\n\n<style scoped>\n/* \u53ef\u6839\u636e\u9700\u8981\u6dfb\u52a0\u6837\u5f0f */\n</style>\n",
        "from_file": "views/LoginPage.vue"
    },
    "views/ManageMenu.vue": {
        "content": "<!-- src/views/ManageMenu.vue -->\n\n<template>\n    <v-container>\n      <h1>\u83dc\u5355\u7ba1\u7406</h1>\n      <v-btn color=\"primary\" @click=\"openAddDialog\">\u6dfb\u52a0\u83dc\u54c1</v-btn>\n  \n      <!-- \u83dc\u5355\u5217\u8868 -->\n      <v-data-table\n        :headers=\"headers\"\n        :items=\"menuItems\"\n        class=\"elevation-1\"\n        :loading=\"loading\"\n        loading-text=\"\u52a0\u8f7d\u4e2d...\"\n      >\n        <template v-slot:item.actions=\"{ item }\">\n          <v-btn icon @click=\"openEditDialog(item)\">\n            <v-icon>mdi-pencil</v-icon>\n          </v-btn>\n          <v-btn icon @click=\"deleteItem(item.item_id)\">\n            <v-icon color=\"red\">mdi-delete</v-icon>\n          </v-btn>\n        </template>\n      </v-data-table>\n  \n      <!-- \u6dfb\u52a0/\u7f16\u8f91\u83dc\u54c1\u5bf9\u8bdd\u6846 -->\n      <v-dialog v-model=\"dialog\" max-width=\"500px\">\n        <v-card>\n          <v-card-title>\n            <span class=\"headline\">{{ dialogTitle }}</span>\n          </v-card-title>\n          <v-card-text>\n            <v-form ref=\"form\" v-model=\"valid\">\n              <v-text-field\n                v-model=\"formData.name\"\n                label=\"\u83dc\u54c1\u540d\u79f0\"\n                :rules=\"[v => !!v || '\u83dc\u54c1\u540d\u79f0\u4e3a\u5fc5\u586b\u9879']\"\n                required\n              ></v-text-field>\n  \n              <v-textarea\n                v-model=\"formData.description\"\n                label=\"\u63cf\u8ff0\"\n              ></v-textarea>\n  \n              <v-text-field\n                v-model=\"formData.price\"\n                label=\"\u4ef7\u683c\"\n                type=\"number\"\n                :rules=\"[v => !!v || '\u4ef7\u683c\u4e3a\u5fc5\u586b\u9879', v => v > 0 || '\u4ef7\u683c\u5fc5\u987b\u5927\u4e8e0']\"\n                required\n              ></v-text-field>\n  \n              <v-text-field\n                v-model=\"formData.image_url\"\n                label=\"\u56fe\u7247 URL\"\n                type=\"url\"\n              ></v-text-field>\n  \n              <v-switch\n                v-model=\"formData.is_available\"\n                label=\"\u662f\u5426\u53ef\u7528\"\n              ></v-switch>\n            </v-form>\n            <!-- \u56fe\u7247\u9884\u89c8 -->\n            <v-img\n              :src=\"formData.image_url || defaultImage\"\n              height=\"200px\"\n              class=\"mt-2\"\n            ></v-img>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn text @click=\"closeDialog\">\u53d6\u6d88</v-btn>\n            <v-btn color=\"primary\" :disabled=\"!valid\" @click=\"submitForm\">\n              \u4fdd\u5b58\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n  \n      <!-- \u5220\u9664\u786e\u8ba4\u5bf9\u8bdd\u6846 -->\n      <v-dialog v-model=\"deleteDialog\" max-width=\"500px\">\n        <v-card>\n          <v-card-title class=\"headline\">\u786e\u8ba4\u5220\u9664</v-card-title>\n          <v-card-text>\n            \u786e\u5b9a\u8981\u5220\u9664\u83dc\u54c1 \"{{ itemToDelete?.name }}\" \u5417\uff1f\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn text @click=\"closeDeleteDialog\">\u53d6\u6d88</v-btn>\n            <v-btn color=\"red\" @click=\"confirmDelete\">\u5220\u9664</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n  \n      <!-- \u6210\u529f\u4e0e\u9519\u8bef\u6d88\u606f -->\n      <v-snackbar v-model=\"snackbar.show\" :color=\"snackbar.color\" timeout=\"6000\">\n        {{ snackbar.message }}\n        <v-btn color=\"white\" text @click=\"snackbar.show = false\">\u5173\u95ed</v-btn>\n      </v-snackbar>\n    </v-container>\n  </template>\n  \n  <script>\n  import { mapGetters, mapActions } from 'vuex'\n  \n  export default {\n    name: 'ManageMenu',\n    data() {\n      return {\n        headers: [\n          { text: '\u540d\u79f0', value: 'name' },\n          { text: '\u63cf\u8ff0', value: 'description' },\n          { text: '\u4ef7\u683c', value: 'price' },\n          { text: '\u56fe\u7247', value: 'image_url' },\n          { text: '\u662f\u5426\u53ef\u7528', value: 'is_available' },\n          { text: '\u64cd\u4f5c', value: 'actions', sortable: false },\n        ],\n        dialog: false,\n        dialogTitle: '',\n        formData: {\n          item_id: null,\n          name: '',\n          description: '',\n          price: '',\n          image_url: '',\n          is_available: true,\n        },\n        valid: false,\n        deleteDialog: false,\n        itemToDelete: null,\n        defaultImage: 'https://via.placeholder.com/200', // \u9ed8\u8ba4\u56fe\u7247\n        loading: false,\n        snackbar: {\n          show: false,\n          message: '',\n          color: '',\n        },\n      }\n    },\n    computed: {\n      ...mapGetters('menu', ['getMenuItems']),\n      menuItems() {\n        return this.getMenuItems\n      },\n    },\n    methods: {\n      ...mapActions('menu', ['fetchMenu', 'addMenuItem', 'updateMenuItem', 'deleteMenuItem']),\n      openAddDialog() {\n        this.dialogTitle = '\u6dfb\u52a0\u83dc\u54c1'\n        this.formData = {\n          item_id: null,\n          name: '',\n          description: '',\n          price: '',\n          image_url: '',\n          is_available: true,\n        }\n        this.dialog = true\n      },\n      openEditDialog(item) {\n        this.dialogTitle = '\u7f16\u8f91\u83dc\u54c1'\n        this.formData = { ...item }\n        this.dialog = true\n      },\n      closeDialog() {\n        this.dialog = false\n      },\n      async submitForm() {\n        if (this.$refs.form.validate()) {\n          try {\n            if (this.formData.item_id) {\n              // \u66f4\u65b0\u83dc\u54c1\n              await this.updateMenuItem(this.formData)\n              this.snackbar.message = '\u83dc\u54c1\u66f4\u65b0\u6210\u529f\uff01'\n              this.snackbar.color = 'success'\n            } else {\n              // \u6dfb\u52a0\u83dc\u54c1\n              await this.addMenuItem(this.formData)\n              this.snackbar.message = '\u83dc\u54c1\u6dfb\u52a0\u6210\u529f\uff01'\n              this.snackbar.color = 'success'\n            }\n            this.snackbar.show = true\n            this.dialog = false\n          } catch (error) {\n            this.snackbar.message = '\u64cd\u4f5c\u5931\u8d25\uff0c\u8bf7\u91cd\u8bd5\u3002'\n            this.snackbar.color = 'error'\n            this.snackbar.show = true\n            console.error('\u63d0\u4ea4\u8868\u5355\u5931\u8d25\uff1a', error)\n          }\n        }\n      },\n      deleteItem(itemId) {\n        this.itemToDelete = this.menuItems.find(item => item.item_id === itemId)\n        this.deleteDialog = true\n      },\n      closeDeleteDialog() {\n        this.deleteDialog = false\n        this.itemToDelete = null\n      },\n      async confirmDelete() {\n        try {\n          await this.deleteMenuItem(this.itemToDelete.item_id)\n          this.snackbar.message = '\u83dc\u54c1\u5220\u9664\u6210\u529f\uff01'\n          this.snackbar.color = 'success'\n          this.snackbar.show = true\n        } catch (error) {\n          this.snackbar.message = '\u5220\u9664\u5931\u8d25\uff0c\u8bf7\u91cd\u8bd5\u3002'\n          this.snackbar.color = 'error'\n          this.snackbar.show = true\n          console.error('\u5220\u9664\u83dc\u54c1\u5931\u8d25\uff1a', error)\n        } finally {\n          this.deleteDialog = false\n          this.itemToDelete = null\n        }\n      },\n    },\n    created() {\n      this.fetchMenu()\n    },\n  }\n  </script>\n  \n  <style scoped>\n  /* \u53ef\u6839\u636e\u9700\u8981\u6dfb\u52a0\u6837\u5f0f */\n  .mt-2 {\n    margin-top: 0.5rem;\n  }\n  </style>\n  ",
        "from_file": "views/ManageMenu.vue"
    },
    "views/SalesStatistics.vue": {
        "content": "<!-- src/views/SalesStatistics.vue -->\n\n<template>\n    <v-container>\n      <h1>\u9500\u552e\u7edf\u8ba1</h1>\n      <!-- \u9500\u552e\u7edf\u8ba1\u529f\u80fd\u5c06\u5728\u540e\u7eed\u9636\u6bb5\u5b9e\u73b0 -->\n    </v-container>\n  </template>\n  \n  <script>\n  export default {\n    name: 'SalesStatistics',\n  }\n  </script>\n  \n  <style scoped>\n  /* \u53ef\u6839\u636e\u9700\u8981\u6dfb\u52a0\u6837\u5f0f */\n  </style>\n  ",
        "from_file": "views/SalesStatistics.vue"
    },
    "views/HomePage.vue": {
        "content": "<!-- src/views/HomePage.vue -->\n\n<template>\n  <v-container>\n    <h1>\u6b22\u8fce\u4f7f\u7528 kkk\u9910\u5385\u8ba2\u9910\u7cfb\u7edf</h1>\n    <div v-if=\"isAuthenticated && user\" class=\"mt-4\">\n      <v-card>\n        <v-card-title>\u5f53\u524d\u7528\u6237\u4fe1\u606f</v-card-title>\n        <v-card-text>\n          <p><strong>\u7528\u6237\u540d\uff1a</strong>{{ user.username }}</p>\n          <p><strong>\u59d3\u540d\uff1a</strong>{{ user.name }}</p>\n          <p><strong>\u89d2\u8272\uff1a</strong>{{ user.role }}</p>\n          <p v-if=\"user.phone_number\"><strong>\u7535\u8bdd\u53f7\u7801\uff1a</strong>{{ user.phone_number }}</p>\n          <p v-if=\"user.address\"><strong>\u5730\u5740\uff1a</strong>{{ user.address }}</p>\n        </v-card-text>\n      </v-card>\n    </div>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\n\nexport default {\n  name: 'HomePage',\n  computed: {\n    ...mapGetters('auth', ['isAuthenticated', 'getUser']),\n    user() {\n      return this.getUser\n    },\n  },\n}\n</script>\n\n<style scoped>\n.mt-4 {\n  margin-top: 1rem;\n}\n</style>\n",
        "from_file": "views/HomePage.vue"
    },
    "views/OrdersPage.vue": {
        "content": "<!-- src/views/OrdersPage.vue -->\n\n<template>\n  <v-container>\n    <h1>\u4e0b\u8ba2\u5355</h1>\n    <v-row>\n      <v-col\n        v-for=\"item in menuItems\"\n        :key=\"item.item_id\"\n        cols=\"12\"\n        sm=\"6\"\n        md=\"4\"\n      >\n        <v-card>\n          <v-img :src=\"item.image_url || defaultImage\" height=\"200px\"></v-img>\n          <v-card-title>{{ item.name }}</v-card-title>\n          <v-card-subtitle>\u4ef7\u683c: \u00a5{{ item.price }}</v-card-subtitle>\n          <v-card-text>\n            <p>{{ item.description }}</p>\n            <v-text-field\n              v-model.number=\"item.quantity\"\n              label=\"\u6570\u91cf\"\n              type=\"number\"\n              min=\"1\"\n              @input=\"validateQuantity(item)\"\n            ></v-text-field>\n          </v-card-text>\n          <v-card-actions>\n            <v-btn color=\"primary\" @click=\"addToCart(item)\">\u6dfb\u52a0\u5230\u8d2d\u7269\u8f66</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-col>\n    </v-row>\n\n    <!-- \u8d2d\u7269\u8f66 -->\n    <v-card class=\"mt-5\">\n      <v-card-title>\u8d2d\u7269\u8f66</v-card-title>\n      <v-card-text>\n        <v-list>\n          <v-list-item\n            v-for=\"cartItem in cart\"\n            :key=\"cartItem.item_id\"\n          >\n            <v-list-item-content>\n              <v-list-item-title>{{ cartItem.name }}</v-list-item-title>\n              <v-list-item-subtitle>\u6570\u91cf: {{ cartItem.quantity }}</v-list-item-subtitle>\n            </v-list-item-content>\n            <v-list-item-action>\n              <v-btn icon @click=\"removeFromCart(cartItem)\">\n                <v-icon color=\"red\">mdi-delete</v-icon>\n              </v-btn>\n            </v-list-item-action>\n          </v-list-item>\n        </v-list>\n        <v-divider></v-divider>\n        <v-row justify=\"end\">\n          <v-col cols=\"12\" class=\"text-right\">\n            <h3>\u603b\u4ef7: \u00a5{{ totalPrice.toFixed(2) }}</h3>\n            <v-form ref=\"orderForm\" v-model=\"valid\" class=\"mt-3\">\n              <!-- \u914d\u9001\u5730\u5740 -->\n              <v-text-field\n                v-model=\"deliveryAddress\"\n                label=\"\u914d\u9001\u5730\u5740\"\n                :rules=\"[v => !!v || '\u914d\u9001\u5730\u5740\u4e3a\u5fc5\u586b\u9879']\"\n                required\n              ></v-text-field>\n              \n              <!-- \u914d\u9001\u65f6\u95f4 -->\n              <v-text-field\n                v-model=\"deliveryTime\"\n                label=\"\u914d\u9001\u65f6\u95f4\"\n                type=\"time\"\n                :rules=\"[v => !!v || '\u914d\u9001\u65f6\u95f4\u4e3a\u5fc5\u586b\u9879']\"\n                required\n              ></v-text-field>\n            </v-form>\n            <v-btn color=\"success\" @click=\"submitOrder\" :disabled=\"!valid || cart.length === 0\">\n              \u63d0\u4ea4\u8ba2\u5355\n            </v-btn>\n          </v-col>\n        </v-row>\n      </v-card-text>\n    </v-card>\n\n    <!-- \u6210\u529f\u4e0e\u9519\u8bef\u6d88\u606f -->\n    <v-snackbar v-model=\"snackbar.show\" :color=\"snackbar.color\" timeout=\"6000\">\n      {{ snackbar.message }}\n      <v-btn color=\"white\" text @click=\"snackbar.show = false\">\u5173\u95ed</v-btn>\n    </v-snackbar>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from 'vuex'\n\nexport default {\n  name: 'OrdersPage',\n  data() {\n    return {\n      cart: [],\n      defaultImage: 'https://via.placeholder.com/200',\n      deliveryAddress: '',\n      deliveryTime: '',\n      valid: false,\n      snackbar: {\n        show: false,\n        message: '',\n        color: '',\n      },\n    }\n  },\n  computed: {\n    ...mapGetters('menu', ['getMenuItems']),\n    ...mapGetters('auth', ['getUser']),\n    menuItems() {\n      return this.getMenuItems\n    },\n    totalPrice() {\n      return this.cart.reduce((total, item) => total + parseFloat(item.price) * item.quantity, 0)\n    },\n  },\n  methods: {\n    ...mapActions('menu', ['fetchMenu']),\n    ...mapActions('orders', ['createOrder']),\n    \n    validateQuantity(item) {\n      if (item.quantity < 1) {\n        item.quantity = 1\n      }\n    },\n\n    addToCart(item) {\n      const existingItem = this.cart.find(cartItem => cartItem.item_id === item.item_id)\n      if (existingItem) {\n        existingItem.quantity += item.quantity\n      } else {\n        this.cart.push({ ...item, quantity: item.quantity || 1 })\n      }\n      this.snackbar.message = '\u5df2\u6dfb\u52a0\u5230\u8d2d\u7269\u8f66\uff01'\n      this.snackbar.color = 'success'\n      this.snackbar.show = true\n    },\n    \n    removeFromCart(item) {\n      this.cart = this.cart.filter(cartItem => cartItem.item_id !== item.item_id)\n      this.snackbar.message = '\u5df2\u4ece\u8d2d\u7269\u8f66\u79fb\u9664\uff01'\n      this.snackbar.color = 'warning'\n      this.snackbar.show = true\n    },\n    \n    async submitOrder() {\n      if (!this.$refs.orderForm.validate()) {\n        return\n      }\n\n      try {\n        const user = this.getUser\n        if (!user) {\n          throw new Error('\u7528\u6237\u672a\u767b\u5f55')\n        }\n\n        // \u786e\u4fdd\u914d\u9001\u65f6\u95f4\u5df2\u9009\u62e9\n        if (!this.deliveryTime) {\n          this.snackbar.message = '\u8bf7\u9009\u62e9\u914d\u9001\u65f6\u95f4\u3002'\n          this.snackbar.color = 'error'\n          this.snackbar.show = true\n          return\n        }\n\n        // \u83b7\u53d6\u5f53\u524d\u65e5\u671f\n        const today = new Date()\n        const year = today.getFullYear()\n        const month = String(today.getMonth() + 1).padStart(2, '0')\n        const day = String(today.getDate()).padStart(2, '0')\n\n        // \u683c\u5f0f\u5316\u65f6\u95f4\uff0c\u786e\u4fdd\u6709\u79d2\u6570\n        const [hours, minutes] = this.deliveryTime.split(':')\n        const combinedDeliveryTime = `${year}-${month}-${day} ${hours}:${minutes}:00`\n\n        const orderData = {\n          customer_id: user.user_id, // \u786e\u4fdd\u5305\u542bcustomer_id\n          delivery_time: combinedDeliveryTime,\n          delivery_address: this.deliveryAddress,\n          items: this.cart.map(item => ({\n            item_id: item.item_id,\n            quantity: item.quantity,\n          })),\n        }\n        await this.createOrder(orderData)\n        this.snackbar.message = '\u8ba2\u5355\u5df2\u63d0\u4ea4\u6210\u529f\uff01'\n        this.snackbar.color = 'success'\n        this.snackbar.show = true\n        this.cart = []\n        this.deliveryAddress = ''\n        this.deliveryTime = ''\n      } catch (error) {\n        this.snackbar.message = '\u8ba2\u5355\u63d0\u4ea4\u5931\u8d25\uff0c\u8bf7\u91cd\u8bd5\u3002'\n        this.snackbar.color = 'error'\n        this.snackbar.show = true\n        console.error('\u8ba2\u5355\u63d0\u4ea4\u5931\u8d25\uff1a', error)\n      }\n    },\n  },\n  created() {\n    this.fetchMenu()\n  },\n}\n</script>\n\n<style scoped>\n.mt-5 {\n  margin-top: 2rem;\n}\n.mt-3 {\n  margin-top: 1rem;\n}\n</style>\n",
        "from_file": "views/OrdersPage.vue"
    },
    "views/MenuPage.vue": {
        "content": "<!-- src/views/MenuPage.vue -->\n\n<template>\n  <v-container>\n    <h1>\u83dc\u5355</h1>\n    <v-row>\n      <v-col\n        v-for=\"item in menuItems\"\n        :key=\"item.item_id\"\n        cols=\"12\"\n        sm=\"6\"\n        md=\"4\"\n      >\n        <v-card>\n          <v-img\n            :src=\"item.image_url || defaultImage\"\n            height=\"200px\"\n          ></v-img>\n          <v-card-title>{{ item.name }}</v-card-title>\n          <v-card-subtitle>\u4ef7\u683c: \u00a5{{ item.price }}</v-card-subtitle>\n          <v-card-text>\n            <p>{{ item.description }}</p>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n    <!-- \u663e\u793a\u9519\u8bef\u4fe1\u606f -->\n    <v-alert v-if=\"error\" type=\"error\" dismissible>\n      {{ errorMessage }}\n    </v-alert>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\n\nexport default {\n  name: 'MenuPage',\n  data() {\n    return {\n      defaultImage: 'https://via.placeholder.com/200', // \u9ed8\u8ba4\u56fe\u7247\n      error: false,\n      errorMessage: '',\n    }\n  },\n  computed: {\n    ...mapGetters('menu', ['getMenuItems']),\n    menuItems() {\n      return this.getMenuItems\n    },\n  },\n  created() {\n    this.fetchMenu()\n  },\n  methods: {\n    async fetchMenu() {\n      try {\n        await this.$store.dispatch('menu/fetchMenu')\n      } catch (error) {\n        this.error = true\n        this.errorMessage = '\u83b7\u53d6\u83dc\u5355\u5931\u8d25\uff0c\u8bf7\u7a0d\u540e\u518d\u8bd5\u3002'\n        console.error('\u83b7\u53d6\u83dc\u5355\u5931\u8d25\uff1a', error)\n      }\n    },\n  },\n}\n</script>\n\n<style scoped>\n/* \u53ef\u6839\u636e\u9700\u8981\u6dfb\u52a0\u6837\u5f0f */\n</style>\n",
        "from_file": "views/MenuPage.vue"
    },
